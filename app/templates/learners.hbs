<div class="learners">
  <div class="containers">
    <div class="search-text">
      <input type="text" class="form-control" placeholder="Search" />
    </div>
    <div class="actions">
      <button type="button" class="search">{{app-icon name='search'}}</button>
      <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>

  </div>
  <div id="world-map">
    {{charts.world-map locationDataCount=geoLocations keyName='total'}}
  </div>
  <div id="global-statistics">
    {{learners.global-statistics subjects=subjects}}
  </div>
</div>
